{
  "rewrites": [
    { "source": "/", "destination": "/dashboard/index.html" },
    { "source": "/prelaunch", "destination": "/prelaunch.html" },
    { "source": "/pricing", "destination": "/pricing.html" },
    { "source": "/thank-you", "destination": "/thank-you.html" },
    { "source": "/case-studies/mikork", "destination": "/case-studies/mikork.html" },
    { "source": "/mikork", "destination": "/case-studies/mikork.html" },
    { "source": "/showcase/mikork", "destination": "/showcase/mikork.html" },
    { "source": "/api/:path*", "destination": "/dashboard/api/:path*" }
  ],
  "headers": [
    {
      "source": "/robots.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain" }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" }
      ]
    },
    {
      "source": "/assets/:path*",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
