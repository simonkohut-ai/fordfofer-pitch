{
  "rewrites": [
    { "source": "/prelaunch", "destination": "/prelaunch.html" },
    { "source": "/pricing", "destination": "/pricing.html" },
    { "source": "/thank-you", "destination": "/thank-you.html" },
    { "source": "/case-studies/mikork", "destination": "/case-studies/mikork.html" },
    { "source": "/mikork", "destination": "/case-studies/mikork.html" },
    { "source": "/showcase/mikork", "destination": "/showcase/mikork.html" },
    { "source": "/showcase/mikork/en", "destination": "/showcase/mikork/en.html" },
    { "source": "/products/local-business-basic", "destination": "/products/local-business-basic.html" },
    { "source": "/products/local-business-care", "destination": "/products/local-business-care.html" },
    { "source": "/products/ai-marketing-starter", "destination": "/products/ai-marketing-starter.html" },
    { "source": "/demo/automated-marketing", "destination": "/demo/automated-marketing.html" },
    { "source": "/api/:path*", "destination": "/dashboard/api/:path*" }
  ],
  "headers": [
    {
      "source": "/robots.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain" }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" }
      ]
    },
    {
      "source": "/assets/:path*",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
