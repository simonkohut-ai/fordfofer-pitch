{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/pricing",
      "dest": "/pricing.html"
    },
    {
      "src": "/prelaunch",
      "dest": "/prelaunch.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

